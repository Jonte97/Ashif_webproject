<div class="container">
    <div class="wrapper">
        <h2 class="text-center">Contact</h2>
        <hr />
        <form class="mt-5">
            <div class="form-group">
                <label for="email">Email address</label>
                <input id="email" type="email" name="E-mail" class="form-control textbox" aria-describedby="emailHelp" placeholder="Enter email" />
                <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
            </div>
            <div class="form-group">
                <label for="subject">Subject</label>
                <input id="subject" type="text" placeholder="Subject" name="subject" class="form-control textbox" />
            </div>
            <div class="form-group">
                <label for="textarea">Message</label>
                <textarea id="textarea" rows="4" placeholder="Message" class="form-control textbox"></textarea>
            </div>
            <button type="submit" class="btn btn-primary">Send</button>
        </form>
    </div>
</div>
